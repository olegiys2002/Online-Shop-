@{
}
@model ListOfUsersViewModel

 
<table>
    <tr> <td> Пользователь</td>  <td>Email</td> </tr>
    
    @foreach(var user in Model.UserDTO)
    {   <div asp-validation-summary="ModelOnly"></div>
        <tr> 
            <td> @user.Name </td>
            <td> @user.Email </td>
            <td> <form method="post" > <button type="submit" asp-route-id=@user.Id asp-controller=User asp-action=Delete > Удалить </button> </form> </td>
        </tr>
    }

</table>

